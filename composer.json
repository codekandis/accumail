{
	"name": "codekandis/accumail",
	"description": "`codekandis/accumail` is a CLI and API application to provide accumulated e-mail distribution.",
	"keywords": [
		"php",
		"accumail",
		"cli",
		"api",
		"accumulated",
		"e-mail",
		"distribution"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/accumail/issues",
		"docs": "https://github.com/codekandis/accumail/README.md",
		"source": "https://github.com/codekandis/accumail/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/tiphy": "^0",
		"codekandis/configurations": "^0",
		"codekandis/sentry-client": "^1",
		"codekandis/tiphy-sentry-client-integration": "^0",
		"codekandis/authentication": "^0",
		"codekandis/tiphy-authentication-integration": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\AccuMail\\": [
				"public/",
				"src/"
			],
			"CodeKandis\\AccuMail\\Configurations\\Plain\\": [
				"config/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\AccuMail\\Tests\\": [
				"tests/"
			]
		}
	}
}
